datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

generator db {
  provider  = "prisma-client-py"
  interface = "sync"
}

model Users{
    id       Int     @id @default(autoincrement())
    username     String   @unique
    email       String   @unique
    password    String
}